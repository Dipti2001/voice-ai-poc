# Copy this file to .env and fill in your own credentials

# Twilio account credentials
TWILIO_ACCOUNT_SID=your_account_sid
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_PHONE_NUMBER=+15550000000

# Deepgram API key for ASR/TTS
DEEPGRAM_API_KEY=your_deepgram_key

# LLM provider configuration
# Supported providers: google, openrouter
LLM_PROVIDER=google
LLM_API_KEY=your_llm_api_key
# For Google AI Studio, use models like 'gemini-2.0-pro' or 'gemini-2.5-flash'
# For OpenRouter, specify the model e.g. 'meta-llama/llama-3-70b-chat' or 'deepseek-ai/deepseek-chat'
LLM_MODEL=gemini-2.0-pro

# Base URL of this server for webhook callbacks (used by Twilio)
APP_BASE_URL=https://your-app.example.com